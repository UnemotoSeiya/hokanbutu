<h1>プロフィール編集</h1>
<%= form_with model: @user do |f| %>
  <h4>プロフィール画像</h4>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>  
  <h4>氏名</h4>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <h4>従業員番号</h4>
  <%= f.text_field :number %>
  <h4>電話番号</h4>
  <%= f.text_field :telephone_number %>
  <h4>メールアドレス</h4>
  <%= f.text_field :email %>
  <div><%= f.submit "保存" %></div>
<% end %>